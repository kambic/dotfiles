StrictHostKeyChecking no

Host *
	# IdentityAgent ~/.1password/agent.sock
	ControlMaster auto
	ControlPath ~/.ssh/sockets/%r@%h-%p
	ControlPersist 600
	ForwardAgent yes
	Compression no
	# SetEnv TERM=xterm-256color
  #ServerAliveInterval 60 # fix of lame internet

Host aur.archlinux.org
	User aur
	#IdentityFile ~/.ssh/aur
Host cobra
	User root
